#include <iostream>
#include <string>
using namespace std;
int main() {
    string s;
    getline(cin, s);
  
    string ans = "";
    string max = "";
    char st = 32;
    for(auto z: s){
        
        if(z > st || z < st){
            ans += z;}
            
        
        if(max.size() < ans.size()){
            max = ans;
        }
                
        if(z == st){
            ans = "";}
        }
    cout << max;
    
    return 0;
}
